#: modules/file/file.field.inc:905
msgid "Files must be less than !size."
msgstr "A dimensão dos ficheiros deve ser inferior a !size."

#: modules/file/file.field.inc:908
msgid "Allowed file types: !extensions."
msgstr "Tipos de ficheiros permitidos: !extensions."

#: modules/file/file.field.inc:914
msgid "Images must be exactly !size pixels."
msgstr "As imagens têm de ter exactamente !size píxeis."

#: modules/file/file.field.inc:917
msgid "Images must be between !min and !max pixels."
msgstr "As imagens têm de ter entre !min e !max píxeis."

#: modules/file/file.field.inc:920
msgid "Images must be larger than !min pixels."
msgstr "As imagens têm de ter mais de !min píxeis."

#: modules/file/file.field.inc:923
